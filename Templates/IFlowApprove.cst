        /// <summary>
        /// 审批通过
        /// </summary>
        /// <param name="guid">业务GUID</param>
        ///<param name="isFormWorkFlow">是否来自工作流审批</param>
        void ApprovePass(Guid guid, bool isFormWorkFlow = false);

        /// <summary>
        /// 审批驳回
        /// </summary>
        /// <param name="guid">业务GUID</param>
        ///<param name="isFormWorkFlow">是否来自工作流审批</param>
        void ApproveReject(Guid guid, bool isFormWorkFlow = false);

    }

}
